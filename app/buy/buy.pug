extends ../layout/_layout.pug
block vars
  - var pageTitle = 'Как купить'
  - var bodyClass = ''

block content
  // Top Panel
  include ../includes/_top-panel-inner
  // Top Panel End
  // SideBar
  include ../includes/_sidebar
  // SideBar End

  // Main
  main.main
    // Section About
    section.section.slide(data-hash='hood')
      .grid
        .row.row--full
          .col.col--left.col--vertical-center
            h2.heading.heading--primary.heading--primary-inner Как купить
            nav.nav.nav--submenu
              ul.nav__list.nav__list--horizontal
                li.nav__item.nav__item--submenu.nav__item--active
                  a.link.link--nav(href='#') Рассрочка
                li.nav__item.nav__item--submenu
                  a.link.link--nav(href='#') ипотека
                li.nav__item.nav__item--submenu
                  a.link.link--nav(href='#') trade-in
                li.nav__item.nav__item--submenu
                  a.link.link--nav(href='#') выкуп авто
            h3.heading.heading--secondary.heading--secondary-inner Рассрочка
            button.btn.btn--base(type='button', name='button') Отправить заявку
          .col.col--center.col--center.col--vertical-center
            h3.heading.heading--secondary Как купить квартиру в «Геометрии»? Легко!
            p.paragraph.text.text--normal.text--regular
              | Есть множество способов приблизить долгожданное новоселье.
              br
              |                 Например, вы можете внести не всю сумму сразу, а по частям:
            ul.list.list--dot
              li.list__item.list__item--dot
                span.text.text--normal.text--regular при первоначальном взносе от 75% — рассрочка без удорожания;
              li.list__item.list__item--dot
                span.text.text--normal.text--regular
                  | при первоначальном взносе от 50% до 75% —
                  br
                  | рассрочка под 1% с ежемесячным удорожанием;
              li.list__item.list__item--dot
                span.text.text--normal.text--regular
                  | при первоначальном взносе от 30% до 50% —
                  br
                  | рассрочка под 1,5% с ежемесячным удорожанием.
            p.paragraph.text.text--normal.text--regular
              | Рассрочка предоставляется до 30 сентября 2019 года.
          .col.col--right(style="background-image: url('../images/installment-right.jpg');")
    // Section About End
    // Section About
    section.section.slide(data-hash='hood')
      .grid
        .row.row--full
          .col.col--left.col--vertical-center
            h2.heading.heading--primary.heading--primary-inner Как купить
            nav.nav.nav--submenu
              ul.nav__list.nav__list--horizontal
                li.nav__item.nav__item--submenu
                  a.link.link--nav(href='#') Рассрочка
                li.nav__item.nav__item--submenu.nav__item--active
                  a.link.link--nav(href='#') ипотека
                li.nav__item.nav__item--submenu
                  a.link.link--nav(href='#') trade-in
                li.nav__item.nav__item--submenu
                  a.link.link--nav(href='#') выкуп авто
            h3.heading.heading--secondary.heading--secondary-inner Ипотека
            button.btn.btn--base(type='button', name='button') Отправить заявку
          .col.col--center.col--center.col--vertical-center
            ul.list.list--unstyled.list--cards
              li.list__item.list__item--card
                .card.card--info.card--banks
                  .card__front
                    .card__info
                      svg.icon.icon--info(xmlns='http://www.w3.org/2000/svg', viewBox='0 0 82 214.24')
                        g#info
                          path(d='M75.42 178.51H70v-94.3a6.48 6.48 0 0 0-6.58-6.38H6.58A6.48 6.48 0 0 0 0 84.21v23a6.48 6.48 0 0 0 6.58 6.38h5.53v65H6.58a6.48 6.48 0 0 0-6.58 6.3v23a6.48 6.48 0 0 0 6.58 6.38h68.84a6.48 6.48 0 0 0 6.58-6.41v-23a6.48 6.48 0 0 0-6.58-6.35zm-6.58 23H13.16v-10.24h5.53a6.48 6.48 0 0 0 6.58-6.38v-77.71a6.48 6.48 0 0 0-6.58-6.38h-5.53V90.59h43.57v94.3a6.48 6.48 0 0 0 6.58 6.38h5.46zM37.71 61.41A30.71 30.71 0 1 0 7 30.71a30.71 30.71 0 0 0 30.71 30.7zm0-46.68a11.35 11.35 0 1 1-.07-.07h.07z')
                          circle(cx='37.71', cy='30.71', r='20.46')
                          path(d='M7 86.18h56.91v19.75H7z')
                          path(d='M17.25 86.18h46.66v121.5H17.25z')
                          path(d='M7.75 183.18h65.66v24.5H7.75z')
                    .card__inner
                      .card__image.clearfix
                        .logo.logo--bank
                          a.logo__link(href='#')
                            img.image.image--logo(src='data:image/png;base64,', alt='')
                      .card__content
                        .grid.grid--inner
                          .row
                            .col.col--auto
                              .text.text--xsmall.text--regular.text--black Годовые проценты
                              span.text.text--normal.text--regular.text--black от
                              span.text.text--xlarge.text--regular.text--black 9,3%
                            .col.col--auto
                              .text.text--xsmall.text--regular.text--black Первоначальный взнос
                              span.text.text--normal.text--regular.text--black от
                              span.text.text--xlarge.text--regular.text--black 15%
                  .card__back
                    .card__inner.bg--grey-dark
                      .info__inner
                        ul.list.list--unstyled
                          li.list__item
                            .text.text--brand.text--xsmall.text--regular возраст от 21 до 65 лет
                          li.list__item
                            .text.text--brand.text--xsmall.text--regular стаж на последнем месте работы 3 мес
                          li.list__item
                            .text.text--brand.text--xsmall.text--regular рассмотрения ИП и собственников бизнеса
                          li.list__item
                            .text.text--brand.text--xsmall.text--regular подтверждение дохода по форме Банка/форме стороннерго банка и в свободной форме
    // Section About End
    // Section About
    section.section.slide(data-hash='hood')
      .grid
        .row.row--full
          .col.col--left.col--vertical-center
            h2.heading.heading--primary.heading--primary-inner Как купить
            nav.nav.nav--submenu
              ul.nav__list.nav__list--horizontal
                li.nav__item.nav__item--submenu
